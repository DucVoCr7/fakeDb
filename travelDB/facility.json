//Facility 
table hotel { 
    id uuid [PK] 
    createAt date 
    updateAt date 
    name string 
    email string 
    password string 
    status tinyint
    level string
    phone number 
    phoneId number
    apartmentId string
    streetId string
    wardId string 
    districtId string 
    provinceId string 
    countryId string
    fullAddress text
    lat float 
    long float 
    lessorId uuid 
    totalRooms number 
    totalRoomsRented number 
    description text 
  } 
  ref: hotel.lessorId > lessor.id

table hotelImage { 
    id uuid [PK] 
    createAt date 
    updateAt date 
    hotelId uuid
  } 
  ref: hotelImage.hotelId > hotel.id

