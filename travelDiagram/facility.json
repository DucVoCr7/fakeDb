//Facility 
table hotel { 
    id uuid [PK] 
    createAt date 
    updateAt date 
    name string 
    email string 
    password string 
    status tinyint
    level tinyint
    phone string 
    phoneId string
    apartmentId string
    streetId string
    wardId string 
    districtId string 
    provinceId string 
    countryId string
    fullAddress text
    lat float 
    long float 
    lessorId uuid 
    totalRooms number 
    totalRoomsRented number 
    description text
    adminApproved uuid
  } 
  ref: hotel.lessorId > lessor.id
  ref: hotel.adminApproved > admin.id

table hotelImage { 
    imageId string [PK] 
    createAt date 
    updateAt date
    image string 
    hotelId uuid
  } 
  ref: hotelImage.hotelId > hotel.id

